{
  "name": "shuheikagawa.com",
  "version": "0.0.0",
  "description": "@shuhei's website",
  "scripts": {
    "start": "eleventy --serve",
    "build": "OPTIMIZE=1 eleventy",
    "clean": "rm -rf public",
    "deploy": "yarn clean && yarn build && gh-pages -d public -b master",
    "lint": "eslint .eleventy.js *.config.js lib cypress source/blog source/posts",
    "serve": "http-server",
    "test": "jest",
    "tdd": "jest --watch",
    "test-text-images": "node lib/filters/manual-tests/title-image.js",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:run:live": "Cypress_baseUrl=https://shuheikagawa.com cypress run",
    "cy:run:open": "Cypress_baseUrl=https://shuheikagawa.com cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shuhei/shuhei.github.com.git"
  },
  "author": "Shuhei Kagawa <shuhei.kagawa@gmail.com>",
  "license": "BSD-2-Clause",
  "private": true,
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">=1.12.0"
  },
  "bugs": {
    "url": "https://github.com/shuhei/shuhei.github.com/issues"
  },
  "homepage": "https://github.com/shuhei/shuhei.github.com",
  "devDependencies": {
    "@11ty/eleventy-cache-assets": "^2.1.0",
    "@11ty/eleventy-img": "^0.7.8",
    "@fontsource/libre-baskerville": "^4.1.0",
    "@fontsource/libre-franklin": "^4.1.0",
    "canvas": "^2.6.1",
    "cypress": "^3.8.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "highlight.js": "^9.11.0",
    "html-minifier": "^4.0.0",
    "http-server": "^0.12.3",
    "image-size": "^0.9.3",
    "jest": "^24.9.0",
    "jsdom": "^16.4.0",
    "markdown-it": "^12.0.4",
    "postcss": "^8.2.4",
    "postcss-clean": "^1.2.1",
    "postcss-custom-properties": "^11.0.0",
    "prettier": "^1.15.3",
    "pug": "^2.0.4"
  },
  "gh-pages": {
    "repository": "shuhei/shuhei.github.com",
    "static": "public",
    "deploy": "_deploy",
    "branch": "master"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1"
  }
}
