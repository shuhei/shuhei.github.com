{
  "name": "shuheikagawa.com",
  "version": "0.0.0",
  "description": "@shuhei's website",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp watch",
    "build": "NODE_ENV=production gulp build",
    "clean": "rm -rf public",
    "deploy": "yarn clean && yarn build && gh-pages -d public -b master",
    "lint": "eslint gulpfile.js plugins source/_layouts",
    "serve": "gulp serve",
    "newpost": "gulp newpost --title",
    "newpage": "gulp newpage --filename",
    "test": "jest",
    "test-text-images": "node plugins/manual-tests/title-image.js",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:run:live": "Cypress_baseUrl=https://shuheikagawa.com cypress run",
    "cy:run:open": "Cypress_baseUrl=https://shuheikagawa.com cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shuhei/shuhei.github.com.git"
  },
  "author": "Shuhei Kagawa <shuhei.kagawa@gmail.com>",
  "license": "BSD-2-Clause",
  "private": true,
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">=1.12.0"
  },
  "bugs": {
    "url": "https://github.com/shuhei/shuhei.github.com/issues"
  },
  "homepage": "https://github.com/shuhei/shuhei.github.com",
  "devDependencies": {
    "@ibm/plex": "^4.0.2",
    "canvas": "^2.6.0",
    "cheerio": "^1.0.0-rc.3",
    "connect": "^3.3.4",
    "cypress": "^3.8.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.5.0",
    "gulp-front-matter": "^1.3.0",
    "gulp-if": "^3.0.0",
    "gulp-markdown": "^5.0.1",
    "gulp-util": "^3.0.3",
    "highlight.js": "^9.11.0",
    "image-size": "^0.8.3",
    "jest": "^24.9.0",
    "mkdirp": "^0.5.0",
    "postcss": "^7.0.7",
    "postcss-clean": "^1.1.0",
    "postcss-custom-properties": "^9.0.2",
    "prettier": "^1.15.3",
    "pug": "^2.0.4",
    "readable-stream": "^3.4.0",
    "serve-static": "^1.9.1",
    "strftime": "^0.10.0",
    "textile-js": "^2.0.4",
    "through2": "^3.0.1",
    "vinyl": "^2.2.0",
    "yargs": "^15.0.2"
  },
  "gh-pages": {
    "repository": "shuhei/shuhei.github.com",
    "static": "public",
    "deploy": "_deploy",
    "branch": "master"
  }
}
